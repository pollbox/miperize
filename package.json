{
  "name": "miperize",
  "version": "0.3.4",
  "description": "MIP up your plain HTML",
  "main": "./lib/miperize",
  "scripts": {
    "test": "mocha $(find test -name '*.test.js')",
    "watch": "mocha --watch $(find test -name '*.test.js')",
    "coverage": "istanbul cover ./node_modules/.bin/_mocha -- $(find test -name '*.test.js')",
    "test-travis": "istanbul cover node_modules/.bin/_mocha --report lcovonly -- $(find test -name '*.test.js')",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pollbox/miperize.git"
  },
  "keywords": [
    "mip",
    "html"
  ],
  "author": {
      "name": "pollbox",
      "email": "pollbox@dingtalk.com",
      "url": "https://ghostblog.cn"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pollbox/miperize/issues"
  },
  "homepage": "https://github.com/pollbox/miperize#readme",
  "dependencies": {
    "async": "2.1.4",
    "emits": "3.0.0",
    "htmlparser2": "3.9.2",
    "image-size": "0.5.1",
    "lodash.merge": "4.6.0",
    "nock": "^9.0.2",
    "rewire": "^2.5.2",
    "uuid": "^3.0.0"
  },
  "devDependencies": {
    "chai": "3.5.0",
    "cz-conventional-changelog": "1.2.0",
    "istanbul": "0.4.5",
    "mocha": "3.2.0",
    "semantic-release": "6.3.2",
    "sinon": "1.17.7",
    "sinon-chai": "2.8.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
